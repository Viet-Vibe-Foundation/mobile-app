{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": ["assets/*"],
      "@custom-types/*": ["src/types/*"],
      "@components/*": ["src/ui/components/*"],
      "@data/*":["src/data/*"],
      "@constants":["src/constants"],
    }
  }
}
